{
  "name": "@morten-olsen/builder-client",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/morten-olsen/builder.git",
    "directory": "packages/client"
  },
  "type": "module",
  "main": "dist/client.js",
  "exports": {
    ".": "./dist/client.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:spec": "tsx ../server/scripts/extract-openapi.ts",
    "generate:types": "openapi-typescript openapi.json -o src/api.generated.ts",
    "generate": "pnpm generate:spec && pnpm generate:types",
    "build": "pnpm generate && tsc --build"
  },
  "dependencies": {
    "openapi-fetch": "^0.14"
  },
  "devDependencies": {
    "@morten-olsen/builder-configs": "workspace:*",
    "@morten-olsen/builder-server": "workspace:*",
    "@types/node": "25.3.0",
    "openapi-typescript": "^7",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  }
}
